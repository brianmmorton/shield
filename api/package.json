{
  "name": "express-postgres-boilerplate",
  "description": "Express Boilerplate",
  "author": "Brian Morton",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/brianmorton/express-postgres-boilerplate"
  },
  "main": "bin/index.js",
  "scripts": {
    "start": "better-npm-run start-prod-api",
    "worker": "node ./bin/worker.js",
    "scheduler": "node ./bin/scheduler.js",
    "lint": "eslint -c .eslintrc src",
    "dev": "better-npm-run start-dev-api",
    "migrate": "sequelize db:migrate"
  },
  "betterScripts": {
    "start-prod-api": {
      "command": "nodemon ./bin/index.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "APIPORT": 3030,
        "BLUEBIRD_WARNINGS": 0
      }
    },
    "start-dev-api": {
      "command": "nodemon ./bin/index.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "APIPORT": 3030,
        "BLUEBIRD_WARNINGS": 0
      }
    }
  },
  "dependencies": {
    "autoprefixer-loader": "^3.1.0",
    "aws-sdk": "^2.226.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0-beta6",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-import": "^1.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "bcrypt": "^1.0.2",
    "better-npm-run": "0.0.8",
    "body-parser": "^1.14.1",
    "chai": "^3.3.0",
    "compression": "^1.6.0",
    "compromise": "^8.0.2",
    "concurrently": "^0.1.1",
    "cookie-parser": "^1.3.5",
    "dotenv": "^5.0.1",
    "eslint": "1.10.3",
    "eslint-config-airbnb": "0.1.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-import": "^0.8.0",
    "eslint-plugin-react": "^3.5.0",
    "express": "^4.13.3",
    "express-session": "^1.12.1",
    "express-validator": "^5.1.2",
    "helmet": "^2.0.0",
    "invariant": "^2.2.0",
    "kue": "git+https://github.com/ross-nordstrom/kue.git#prevent_duplicate_jobs#1033",
    "lodash": "^4.15.0",
    "minimist": "^1.2.0",
    "mocha": "^2.3.3",
    "moment-timezone": "https://registry.npmjs.org/moment-timezone/-/moment-timezone-0.5.13.tgz",
    "mongoose": "^5.0.15",
    "mongoose-timezone": "^1.1.0",
    "morgan": "^1.7.0",
    "multer": "^1.3.0",
    "nodemon": "^1.11.0",
    "passport": "^0.3.2",
    "passport-jwt": "^4.0.0",
    "pg": "^6.0.1",
    "pg-native": "^1.10.0",
    "pg-parse-float": "0.0.1",
    "piping": "^0.3.0",
    "pretty-error": "^1.2.0",
    "sequelize": "^3.23.4",
    "sequelize-cli": "^2.4.0",
    "serialize-javascript": "^1.1.2",
    "serve-favicon": "^2.3.0",
    "sinon": "^1.17.2",
    "validator": "^8.0.0",
    "warning": "^2.1.0"
  },
  "engines": {
    "node": "8.0.0"
  }
}
